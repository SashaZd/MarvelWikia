    "wiki": {
        "universe": "[[Marvel Universe]]",
        "real_name": "Tabitha Smith",
        "identity": "Secret",
        "occupation": "Adventurer, former student, thief",
        "place_of_birth": "Roanoke, Virginia",
        "groups": "[[X-Men]]; formerly [[Nextwave]], [[X-Force]], Genoshan Mutates, [[New Mutants]], X-Terminators / [[X-Factor]] (trainee), [[Fallen Angels]]",
