    "wiki": {
        "universe": "[[Earth-2189]]",
        "real_name": "Namora",
        "identity": "Publicly known",
        "occupation": "Ruler of Earth, Queen of Atlantis, reality traveler",
        "place_of_birth": "Atlantic Ocean",
